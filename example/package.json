{
	"name": "example-tsc-codebase",
	"scripts": {
		"build": "mkdir -p out && ./node_modules/typescript/bin/tsc -p src && webpack --config webpack.config.js",
		"dev": "npm run build && npm run serve",
		"lsp:analyse": "customelements-analyse --src='src/**/*.{js,ts}' --lib='node_modules/example-lib/**/custom-elements.json' --dependencies",
		"serve": "http-server -c-1 ./public"
	},
	"dependencies": {
		"@genesiscommunitysuccess/custom-elements-lsp": "file:..",
		"@microsoft/fast-components": "2.21.3",
		"@microsoft/fast-element": "1.11.0",
		"@microsoft/fast-foundation": "2.47.0",
		"example-lib": "file:../example-lib",
		"http-serve": "^1.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.4.3",
		"webpack": "^5.70.0"
	},
	"devDependencies": {
		"html-webpack-plugin": "^5.5.1",
		"webpack-cli": "^4.9.2"
	}
